{
  "name": "mosahayaBack",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "12.x.x",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "startlocal": "npm run build && node -r dotenv/config dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon --watch src --exec ts-node -r dotenv/config ./src/index.ts",
    "dev": "ts-node-dev -r dotenv/config --respawn --transpile-only ./src/index.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "tsc && npm run lint && npm run delete && npm run copy",
    "deploy": "npm run build && gcloud app deploy app.yaml --project mosahay-production --stop-previous-version",
    "deploytest": "npm run build && gcloud app deploy app-test.yaml --project mosahay-testing --stop-previous-version",
    "copy": "copyfiles -u 1 \"src/public/**/*\" dist",
    "delete": "del-cli \"dist/public/**/*\""
  },
  "author": "subrat kumar gantayat",
  "license": "ISC",
  "dependencies": {
    "@efstajas/validate-env": "^0.3.3",
    "@google-cloud/logging-winston": "^3.0.6",
    "@google-cloud/pubsub": "^2.2.0",
    "@hapi/boom": "^9.1.0",
    "@hapi/hapi": "^19.1.1",
    "@hapi/inert": "^6.0.1",
    "@hapi/joi": "^17.1.1",
    "@hapi/vision": "^6.0.0",
    "argon2": "^0.27.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "csv-parser": "^2.3.3",
    "firebase-admin": "^9.1.1",
    "good": "^8.1.2",
    "good-console": "^8.0.0",
    "good-squeeze": "^5.1.0",
    "hapi-auth-jwt2": "^10.1.0",
    "hapi-auth-multiple-strategies": "^3.0.0",
    "hapi-geo-locate": "^4.1.0",
    "hapi-good-winston": "^3.0.0",
    "hapi-rate-limit": "^5.0.0",
    "hapi-swagger": "^13.0.0",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.11",
    "randomatic": "^3.1.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/hapi__hapi": "^19.0.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/request-promise": "^4.1.46",
    "copyfiles": "^2.3.0",
    "del-cli": "^3.0.1",
    "dotenv": "^8.2.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.1",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "^3.8.3",
    "ts-node": "^8.10.2",
    "nodemon": "^2.0.4"
  }
}
